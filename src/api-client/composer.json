{
    "name": "hasura-extra/api-client",
    "license": "MIT",
    "description": "The library provides an api client to interact with Hasura api.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Hasura\\ApiClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hasura\\ApiClient\\Tests\\": "tests"
        }
    },
    "conflict": {
        "hasura-extra/hasura-bundle": "*",
        "hasura-extra/sailor-bridge": "<2.3.0",
        "hasura-extra/auth-hook": "<2.3.0",
        "hasura-extra/event-dispatcher": "<2.3.0",
        "hasura-extra/bundle": "<2.3.0",
        "hasura-extra/graphqlite-bridge": "<2.3.0",
        "hasura-extra/metadata": "<2.3.0",
        "hasura-extra/laravel": "<2.3.0"
    },
    "require": {
        "php": ">=8.0",
        "symfony/http-client": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
