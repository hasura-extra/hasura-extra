{
    "name": "hasura-extra/auth-hook",
    "license": "MIT",
    "description": "Webhook authentication for Hasura.",
    "autoload": {
        "psr-4": {
            "Hasura\\AuthHook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hasura\\AuthHook\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-handler": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "nyholm/psr7": "^1.0"
    },
    "conflict": {
        "hasura-extra/sailor-bridge": "<2.0.0",
        "hasura-extra/event-dispatcher": "<2.0.0",
        "hasura-extra/api-client": "<2.0.0",
        "hasura-extra/bundle": "<2.0.0",
        "hasura-extra/graphqlite-bridge": "<2.0.0",
        "hasura-extra/metadata": "<2.0.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-main": "2.1-dev"
        }
    }
}
