{
    "name": "hasura-extra/auth-hook",
    "license": "MIT",
    "description": "Webhook authentication for Hasura.",
    "autoload": {
        "psr-4": {
            "Hasura\\AuthHook\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hasura\\AuthHook\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.2",
        "psr/http-factory": "^1.0",
        "psr/http-server-handler": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "nyholm/psr7": "^1.0"
    },
    "conflict": {
        "hasura-extra/event-dispatcher": "<5.0.0",
        "hasura-extra/api-client": "<5.0.0",
        "hasura-extra/bundle": "<5.0.0",
        "hasura-extra/metadata": "<5.0.0",
        "hasura-extra/graphql-scalars": "<5.0.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
