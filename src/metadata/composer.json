{
    "name": "hasura-extra/metadata",
    "license": "MIT",
    "description": "The library help to manage Hasura metadata.",
    "autoload": {
        "psr-4": {
            "Hasura\\Metadata\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hasura\\Metadata\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/hasura-metadata"
    ],
    "conflict": {
        "hasura-extra/hasura-bundle": "*",
        "hasura-extra/auth-hook": "<5.0.0",
        "hasura-extra/event-dispatcher": "<5.0.0",
        "hasura-extra/bundle": "<5.0.0",
        "hasura-extra/laravel": "<2.5.0",
        "hasura-extra/graphql-scalars": "<5.0.0"
    },
    "require": {
        "php": ">=8.2",
        "symfony/console": "^7.0",
        "symfony/filesystem": "^7.0",
        "symfony/string": "^7.0",
        "symfony/yaml": "^7.0",
        "hasura-extra/api-client": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
