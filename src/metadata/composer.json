{
    "name": "hasura-extra/metadata",
    "license": "MIT",
    "description": "The library help to manage Hasura metadata.",
    "autoload": {
        "psr-4": {
            "Hasura\\Metadata\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hasura\\Metadata\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/hasura-metadata"
    ],
    "conflict": {
        "hasura-extra/hasura-bundle": "*",
        "hasura-extra/sailor-bridge": "<2.3.0",
        "hasura-extra/auth-hook": "<2.3.0",
        "hasura-extra/event-dispatcher": "<2.3.0",
        "hasura-extra/bundle": "<2.3.0",
        "hasura-extra/graphqlite-bridge": "<2.3.0",
        "hasura-extra/laravel": "<2.3.0"
    },
    "require": {
        "php": ">=8.0",
        "symfony/console": "^5.0",
        "symfony/filesystem": "^5.0",
        "symfony/string": "^5.0",
        "symfony/yaml": "^5.0",
        "hasura-extra/api-client": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
