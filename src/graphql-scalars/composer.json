{
    "name": "hasura-extra/graphql-scalars",
    "license": "MIT",
    "description": "The library provides scalar types compatible with Hasura graphql engine.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Hasura\\GraphQLScalars\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hasura\\GraphQLScalars\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.2",
        "webonyx/graphql-php": "^15.7",
        "symfony/uid": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.1-dev"
        }
    },
    "conflict": {
        "hasura-extra/event-dispatcher": "<4.0.0",
        "hasura-extra/api-client": "<4.0.0",
        "hasura-extra/auth-hook": "<4.0.0",
        "hasura-extra/metadata": "<4.0.0",
        "hasura-extra/bundle": "<4.0.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
