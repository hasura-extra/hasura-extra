{
    "name": "hasura-extra/event-dispatcher",
    "license": "MIT",
    "description": "Support to dispatch events triggered by Hasura.",
    "autoload": {
        "psr-4": {
            "Hasura\\EventDispatcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hasura\\EventDispatcher\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=8.2",
        "psr/event-dispatcher": "^1.0",
        "psr/http-server-handler": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "nyholm/psr7": "^1.0"
    },
    "conflict": {
        "hasura-extra/auth-hook": "<5.0.0",
        "hasura-extra/api-client": "<5.0.0",
        "hasura-extra/bundle": "<5.0.0",
        "hasura-extra/metadata": "<5.0.0",
        "hasura-extra/laravel": "<2.5.0",
        "hasura-extra/graphql-scalars": "<5.0.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "5.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
