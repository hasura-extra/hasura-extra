{
    "name": "hasura-extra/bundle",
    "license": "MIT",
    "description": "Symfony bundle to build modern web APIs with Hasura graphql engine.",
    "autoload": {
        "psr-4": {
            "Hasura\\Bundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hasura\\Bundle\\Tests\\": "tests",
            "Hasura\\Bundle\\Tests\\Fixture\\App\\": "tests/Fixture/src"
        }
    },
    "require": {
        "php": ">=8.0",
        "hasura-extra/auth-hook": "^2.1",
        "hasura-extra/api-client": "^2.1",
        "hasura-extra/metadata": "^2.1",
        "hasura-extra/event-dispatcher": "^2.1",
        "hasura-extra/graphqlite-bridge": "^2.1",
        "hasura-extra/sailor-bridge": "^2.1",
        "thecodingmachine/graphqlite-bundle": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/framework-bundle": "^5.0",
        "symfony/psr7-pack": "^1.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.4",
        "phpunit/phpunit": "^9.5",
        "symfony/maker-bundle": "^1.0",
        "symfony/orm-pack": "^2.1",
        "symfony/test-pack": "^1.0",
        "symfony/uid": "^5.0",
        "symfony/filesystem": "^5.0",
        "symfony/security-bundle": "^5.0"
    },
    "conflict": {
        "hasura-extra/hasura-bundle": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-main": "2.1-dev"
        }
    }
}
