{
    "name": "hasura-extra/bundle",
    "license": "MIT",
    "description": "Symfony bundle to build modern web APIs with Hasura graphql engine.",
    "autoload": {
        "psr-4": {
            "Hasura\\Bundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hasura\\Bundle\\Tests\\": "tests",
            "Hasura\\Bundle\\Tests\\Fixture\\App\\": "tests/Fixture/src"
        }
    },
    "require": {
        "php": ">=8.0",
        "hasura-extra/auth-hook": "^3.5",
        "hasura-extra/api-client": "^3.5",
        "hasura-extra/metadata": "^3.5",
        "hasura-extra/event-dispatcher": "^3.5",
        "hasura-extra/graphqlite-bridge": "^3.5",
        "hasura-extra/sailor-bridge": "^3.5",
        "thecodingmachine/graphqlite-bundle": "^5.4",
        "symfony/http-kernel": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/psr7-pack": "^1.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.4",
        "phpunit/phpunit": "^9.5",
        "symfony/maker-bundle": "^1.0",
        "symfony/orm-pack": "^2.1",
        "symfony/test-pack": "^1.0",
        "symfony/uid": "^6.0",
        "symfony/filesystem": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/proxy-manager-bridge": "^6.0"
    },
    "conflict": {
        "hasura-extra/hasura-bundle": "*",
        "hasura-extra/laravel": "<2.5.0",
        "hasura-extra/graphql-scalars": "<3.4.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
